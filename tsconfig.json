{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "types": [
      "react",
      "react-native",
      "react-native-get-random-values"
    ]
  },
  "include": [
    "src",
    "src/types"
  ],
  "exclude": [
    "node_modules",
    "android",
    "ios"
  ]
}